{% extends 'base.html.twig' %}

{% block body %}
    <form method="post">
        {{ form_widget(form._token) }}
        <div class="row">
            <div class="form-group col-md-6">
                {{ form_row(form.nom) }}
            </div>
            <div class="form-group col-md-6">
                {{ form_row(form.prenom) }}
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                {{ form_row(form.email) }}
            </div>
            <div class="form-group col-md-6">
                {{ form_row(form.tel) }}
            </div>
        </div>
        <div class="mt-2 text-end">
            <button class="btn btn-primary">Sauver</button>
        </div>
    </form>
{% endblock %}

{% block title %}

{% endblock %}

